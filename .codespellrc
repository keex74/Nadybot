[codespell]
skip = .vscode,debian,data,cache,logs,vendor,html,*.git*,tags,*.sql,50.txt,*.csv,*.temp,*.stub,*.neon
ignore-words-list = ot,ether,vektor,keep-alives,ist,fulfilment,fulfil,fullfil,fulfill,fullfill,hastable,ore,counsil,offen,thar,spacial,pris,crystalised,crystalized,notin,notIn,newLetter
ignore-regex = ([\'"]/.+|\WND\W|\W!nd\W)
